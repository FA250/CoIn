<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="titleTextColor">@color/colorNegro</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="TextAppearance.AppCompat.Widget.ActionBar.Title." parent="@style/Base.TextAppearance.AppCompat">
        <item name="android:textColor">@color/colorNegro</item>

    </style>



    <style name="AppTheme2" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorAccent</item>
        <item name="colorPrimaryDark">@color/colorAccentDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
    </style>

</resources>
